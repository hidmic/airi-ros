<launch>
  <node pkg="move_base" type="move_base" name="move_base" output="screen" required="true">
    <rosparam file="$(find airi_nav)/config/global_costmap_params.yaml" command="load"/>
    <rosparam file="$(find airi_nav)/config/global_planner_params.yaml" command="load"/>
    <rosparam file="$(find airi_nav)/config/local_costmap_params.yaml" command="load"/>
    <rosparam file="$(find airi_nav)/config/local_planner_params.yaml" command="load"/>
    <rosparam file="$(find airi_nav)/config/move_base_params.yaml" command="load"/>
  </node>
</launch>
